{
  "nginx":{
    "start":"docker start wwwserver",
    "stop": "docker stop wwwserver",
    "kill": "docker stop -f wwwserver",
    "restart": "docker restart wwwserver",
    "status": "docker ps |grep wwwserver"
  },
  "svnserve":{
